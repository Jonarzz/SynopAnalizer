<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.40" fx:controller="com.nwpi.view.SynopAnalizerController">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button fx:id="analizeFileButton" mnemonicParsing="false" onAction="#initialize" prefHeight="30.0" prefWidth="100.0" text="Analize a file" textAlignment="CENTER">
               <VBox.margin>
                  <Insets left="100.0" top="65.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="analizeFoldersButton" mnemonicParsing="false" onAction="#initialize" prefHeight="30.0" prefWidth="100.0" text="Analize folders" textAlignment="CENTER">
               <VBox.margin>
                  <Insets left="100.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
